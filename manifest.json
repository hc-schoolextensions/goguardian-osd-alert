{
  "manifest_version": 3,
  "name": "OSD Alert",
  "description": "Alerts a user when they are not on a school device.",
  "author": "Hayden C",
  "version": "0.1",
  "icons": { "128": "icon.png" },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["osd.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "update_url": "https://osd-alert-extension.pages.dev/extension.xml",
  "key": ""
}
